<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Shopping</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/scripts/modernizr-2.6.2.js"></script>
    <link href="~/Content/style.css" rel='stylesheet' type='text/css' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    
    <!----webfonts---->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    <!----//webfonts---->
    <!----start-alert-scroller---->
    <script src="~/scripts/jquery.min.js"></script>
    <script type="text/javascript" src="~/scripts/jquery.easy-ticker.js"></script>
    
    
    <!----start-alert-scroller---->
    <!-- start menu -->
    <link href="~/Content/megamenu.css" rel="stylesheet" type="text/css" media="all" />
    <script type="text/javascript" src="~/scripts/megamenu.js"></script>
    
    <script src="~/scripts/menu_jquery.js"></script>
    <!-- //End menu -->
    <!---slider---->
    <link rel="stylesheet" href="~/Content/slippry.css">
    <script src="~/scripts/jquery-ui.js" type="text/javascript"></script>
    <script src="~/scripts/scripts-f0e4e0c2.js" type="text/javascript"></script>
    
    <!----start-pricerage-seletion---->
    <script type="text/javascript" src="~/scripts/jquery-ui.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/jquery-ui.css">
    
    <!----//End-pricerage-seletion---->
    <!---move-top-top---->
    <script type="text/javascript" src="~/scripts/move-top.js"></script>
    <script type="text/javascript" src="~/scripts/easing.js"></script>
    
</head>
<body>
    
    <!---start-header---->
    <div class="header">
        <div class="top-header">
            <div class="wrap">

                
                <div class="top-header-left">
                            @Html.Partial("_CarritoBillPartial")
                 </div>
                    <div class="top-header-right">
                        
                            @Html.Partial("_LoginPartial")
                        
                    </div>
                <div class="clear"> </div>
                </div>
        </div>
        </div>
        <!----start-mid-head---->
        <div class="mid-header">
            <div class="wrap">
                <div class="mid-grid-left">
                    <form>
                        <input type="text" placeholder="What Are You Looking for?" />
                    </form>
                </div>
                <div class="mid-grid-right">
                    <a class="logo" href="/Home/index.html"><img src="~/Content/Images/logo2.png" title="" /></a>
                </div>
                <div class="clear"> </div>
            </div>
        </div>
        <!----//End-mid-head---->
        
        @*@{
            Html.RenderAction("_GenresList", "Shared");
        }*@
  @*     @Html.Partial("_GenresList")*@
        <!----start-bottom-header---->
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Shopping</p>
        </footer>
    </div>  

    <script src="~/scripts/jquery-1.10.2.min.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
</body>
</html>